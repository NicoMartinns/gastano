<div id="<%= dom_id gasto %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Titulo:</strong>
    <%= gasto.titulo %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Valor:</strong>
    <%= gasto.valor %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Data:</strong>
    <%= gasto.data %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Categoria:</strong>
    <%= gasto.categoria %>
  </div>
</div>
